<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ratings"
    app:startDestination="@id/ratingsFragment">
    <fragment
        android:id="@+id/ratingsFragment"
        android:name="com.leaird.fetchem.activities.RatingsFragment"
        android:label="RatingsFragment"
        tools:layout="@layout/fragment_ratings">
        <argument
            android:name="show_tool_bar"
            android:defaultValue="true"
            app:argType="boolean" />
        <argument
            android:name="show_bottom_navigation"
            android:defaultValue="true"
            app:argType="boolean" />
        <argument
            android:name="toolbar_title"
            android:defaultValue="@string/toolbar_title_ratings"
            app:argType="reference" />
    </fragment>
</navigation>
